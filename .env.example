# Discord Bot Configuration
DISCORD_TOKEN=your_discord_token
BOT_OWNER_ID=your_discord_user_id

# AI Provider Selection (default: ollama)
AI_PROVIDER=groq

# Ollama Configuration (local LLM, no API key needed)
OLLAMA_ENABLED=true
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=llama3.2:1b

# Hugging Face Configuration
HF_TOKEN=your_huggingface_token
HF_MODEL=meta-llama/Meta-Llama-3-8B-Instruct

# Groq Configuration (used for Tavily agent)
GROQ_API_KEY=your_groq_api_key
GROQ_MODEL=meta-llama/llama-4-scout-17b-16e-instruct

# OpenAI Configuration (for image generation)
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4o-mini

# xAI Configuration
XAI_API_KEY=your_xai_api_key
XAI_MODEL=grok-beta

# DeepSeek Configuration
DEEPSEEK_API_KEY=your_deepseek_api_key
DEEPSEEK_MODEL=deepseek-chat

# Anthropic Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key
ANTHROPIC_MODEL=claude-3-haiku-20240307

# Tavily Search Configuration (for web searches)
TAVILY_API_KEY=your_tavily_key

# AI Settings
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=1024
AI_RETRIES=3

# Bot Behavior
MAX_CHAT_HISTORY=20
SIMILARITY_THRESHOLD=0.65
MEMORY_CLEANUP_INTERVAL=3600
INACTIVE_CHANNEL_TIMEOUT=86400
MAX_MESSAGE_LENGTH=1900